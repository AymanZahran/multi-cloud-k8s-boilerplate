    "aws": [
      {
        "access_key": "",
        "region": "us-east-1",
        "secret_key": ""
      }
    ],
    "azurerm": [
      {
        "client_id": "",
        "client_secret": "",
        "features": {
        },
        "subscription_id": "",
        "tenant_id": ""
      }
    ],
    "kubernetes": [
      {
        "alias": "eks-kubernetes",
