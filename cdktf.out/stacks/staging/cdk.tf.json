      "storage_profile_snapshot_controller_enabled": "${var.aks_storage_profile_snapshot_controller_enabled}",
      "tags": "${var.aks_tags}",
      "version": "7.2.0",
      "vnet_subnet_id": "${module.aks_vnet_CD5E95BE.vnet_subnets}"
    },
    "aks_vnet_CD5E95BE": {
