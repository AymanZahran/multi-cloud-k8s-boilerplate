      "cluster_name": "${var.aks_cluster_name}",
      "depends_on": [
        "module.aks_vnet_CD5E95BE"
      ],
      "enable_auto_scaling": "${var.aks_enable_auto_scaling}",
      "key_vault_secrets_provider_enabled": "${var.aks_key_vault_secrets_provider_enabled}",
