      "subnet_ids": [
        "${module.eks_vpc_924ECCC8.public_subnets}"
      ],
      "tags": "${var.eks_tags}",
      "version": "19.15.3",
      "vpc_id": "${module.eks_vpc_924ECCC8.vpc_id}"
