    "eks_B1A34D59": {
      "//": {
        "metadata": {
          "path": "staging/eks/eks",
          "uniqueId": "eks_B1A34D59"
        }
      },
      "cluster_name": "${var.eks_cluster_name}",
      "cluster_tags": "${var.eks_tags}",
